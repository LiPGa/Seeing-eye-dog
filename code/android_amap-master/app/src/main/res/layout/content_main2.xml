<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.a60213.getyourlocation.Main2Activity"
    tools:showIn="@layout/activity_main2">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="information"
        android:id="@+id/text_2_info"
        android:layout_below="@+id/title_2_position_track"
        android:layout_centerHorizontal="true"
        android:layout_alignLeft="@+id/title_2_position_track"
        android:layout_alignRight="@+id/title_2_position_track"
        android:gravity="center"/>

    <TextView
        android:id="@+id/title_2_position_track"
        android:layout_width="wrap_content"
        android:gravity="center"
        android:layout_height="wrap_content"
        android:textColor="@color/lightblue"
        android:text="Position Track"
        android:textSize="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <EditText
        android:id="@+id/edittext_2_latitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:text="53433254234"
        android:textColor="@color/black"
        android:gravity="center"
        android:enabled="false"
        android:layout_below="@+id/text_2_info"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="34dp"
        android:layout_toEndOf="@+id/title_2_longitude" />

    <TextView
        android:id="@+id/title_2_longitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textSize="30dp"
        android:text="Longitude"
        android:layout_below="@+id/edittext_2_latitude"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:text="7654253523"
        android:textColor="@color/black"
        android:gravity="center"
        android:id="@+id/edittext_2_longitude"
        android:enabled="false"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/title_2_longitude"
        android:layout_alignBottom="@+id/title_2_longitude"
        android:layout_below="@+id/edittext_2_latitude" />

    <TextView
        android:id="@+id/title_2_latitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:gravity="center"
        android:text="Latitude"
        android:layout_above="@+id/edittext_2_longitude"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/edittext_2_latitude"
        android:layout_alignTop="@+id/edittext_2_latitude" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="message"
        android:id="@+id/text_2_msg"
        android:gravity="center"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/text_2_info"
        android:layout_alignStart="@+id/text_2_info"
        android:layout_alignEnd="@+id/text_2_info" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:gravity="center"
        android:inputType="number"
        android:id="@+id/edittext_2_userid"
        android:textColor="@color/black"
        android:layout_alignBottom="@+id/title_2_userid"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/title_2_userid" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10"
        android:gravity="center"
        android:inputType="number"
        android:id="@+id/edittext_2_interval"
        android:textColor="@color/black"
        android:layout_alignBaseline="@+id/title_2_timeinterval"
        android:layout_alignBottom="@+id/title_2_timeinterval"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/edittext_2_userid" />

    <Button
        android:id="@+id/button_2_start_gps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="     start GPS     "
        android:background="@color/lightgray"
        android:textStyle="bold"
        android:textColor="@color/aliceblue"
        android:textSize="20dp"
        android:onClick="onStartGPSClick"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/button_2_stop_gps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="     stop   GPS     "
        android:background="@color/lightgray"
        android:textStyle="bold"
        android:textColor="@color/aliceblue"
        android:textSize="20dp"
        android:onClick="onStopGPSClick"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/title_2_userid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textSize="30dp"
        android:text="UserID"
        android:layout_below="@+id/text_2_count_down"
        android:layout_alignParentStart="true"
        android:layout_marginTop="33dp"
        android:layout_alignEnd="@+id/title_2_timeinterval" />

    <TextView
        android:id="@+id/title_2_timeinterval"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textSize="30dp"
        android:text="Time interval"
        android:layout_below="@+id/title_2_userid"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/button_2_show_in_amap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="    show  amap   "
        android:background="@color/lightgray"
        android:textStyle="bold"
        android:textColor="@color/aliceblue"
        android:textSize="20dp"
        android:onClick="onShowAmapClick"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10"
        android:textSize="50dp"
        android:id="@+id/text_2_count_down"
        android:layout_centerVertical="true"
        android:layout_toStartOf="@+id/edittext_2_longitude" />

    <Button
        android:id="@+id/button_2_show_path"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="   Show   Path   "
        android:background="@color/lightgray"
        android:textStyle="bold"
        android:textColor="@color/aliceblue"
        android:textSize="20dp"
        android:onClick="onShowPathClick"
        android:layout_above="@+id/title_2_userid"
        android:layout_alignParentEnd="true" />

</RelativeLayout>
